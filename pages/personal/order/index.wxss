.no-order {
    font-size: 28rpx;
    line-height: 40rpx;
    text-align: center;
    color: var(--neutral-color4);
    margin-top: 40%;
}
.order-list-wrap {
    display: flex;
    flex-direction:column;
    padding: 32rpx 0;
}
.order-list{
    box-sizing: border-box;
    height: 196rpx;
    display: flex;
    justify-content: space-between;
    margin-bottom: 16rpx;
    background-size: 100% 100%;
    background: #F9F6F2;
}

.mouth-coach{
    background-image: url('data:image/png;base64,');
}
.order-list-text {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    padding: 32rpx 10rpx 32rpx 32rpx;
}
.order-list-money {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    padding: 32rpx 32rpx 32rpx 10rpx;
}
.pink {
    background:#EED5D2;
}
.lightgreen{
    background:#C8D67A;
}
.deepgreen{
    background:#75855E;
}
.brown{
    background:#B2A691;
}
.clip{
  clip-path: polygon(0 0, 100% 0, 88% 100%, 0% 100%);
  width: 65%;

}
.order-type {
    font-size: 32rpx;
    font-weight: 500;
    color: var( --black-color);
    line-height: 48rpx;
}
.order-time {
    font-size: 32rpx;
    color: var( --black-color);
    line-height: 48rpx;
}
.order-status {
    color: #71716F;
    font-size: 28rpx;
    line-height:48rpx;
    text-align: end;
}
.order-money{
    font-size: 38rpx;
    line-height: 60rpx;
    font-weight: 500;
    color: var(--sub-color3);
    text-align: end;
}