    "resolveAlias": {
        "@utils/*": "utils/*",
        "@src/*": "src/*"
    },
    "tabBar": {
        "custom": true,
        "color": "#AEAEAB",
        "selectedColor": "#282826",
        "backgroundColor": "#FFFFFF",
        "list": [
            {   
                "pagePath": "pages/index/index",
                "iconPath":"/assets/icons/home.png",
                "selectedIconPath":"/assets/icons/home-active.png",
                "text": "首页"
            },
            {
                "pagePath": "pages/course/index",
                "iconPath":"/assets/icons/course.png",
                "selectedIconPath":"/assets/icons/course-active.png",
                "text": "约课"
            },
            {
                "pagePath": "pages/contracted/index",
                "iconPath":"/assets/icons/contracted.png",
                "selectedIconPath":"/assets/icons/contracted-active.png",
                "text": "已约"
            },
            {
                "pagePath": "pages/card/index",
                "iconPath":"/assets/icons/card.png",
                "selectedIconPath":"/assets/icons/card-active.png",
                "text": "会员卡"
            },
            {
                "pagePath": "pages/personal/index",
                "iconPath":"/assets/icons/personal.png",
                "selectedIconPath":"/assets/icons/personal-active.png",
